{
  "name": "react-tag-select",
  "version": "0.1.0",
  "description": "Tag Component",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/ReactTagSelect.js --config webpack.development.js",
    "build:umd:min": "webpack src/index.js dist/ReactTagSelect.min.js --config webpack.production.js",
    "dev": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --colors --port 8090",
    "lint": "eslint src",
    "clean": "rimraf dist lib",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "tags",
    "tag input",
    "react-component",
    "autosuggest"
  ],
  "author": "Jonathan Thomas",
  "license": "MIT",
  "repository": "https://github.com/captainill/react-tag-select",
  "dependencies": {
    "lodash": "^3.10.1"
  },
  "peerDependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "devDependencies": {
    "babel": "^5.3.3",
    "babel-loader": "^5.1.2",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.13.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.4.2",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "rimraf": "^2.5.1",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.9.4",
    "webpack-dev-server": "^1.8.2"
  }
}
